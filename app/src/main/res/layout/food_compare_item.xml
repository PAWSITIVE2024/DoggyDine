<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="wrap_content"
    android:layout_height="match_parent"
    android:background="#f5f5f5">
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <LinearLayout
            android:layout_width="136dp"
            android:layout_height="match_parent"
            android:orientation="horizontal"
            android:background="#f5f5f5">
            <LinearLayout
                android:layout_width="136dp"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <LinearLayout
                    android:id="@+id/fc_profilelayout"
                    android:layout_width="134dp"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:background="@color/white">
                    <ImageButton
                        android:layout_marginTop="10dp"
                        android:id="@+id/fc_profile_image"
                        android:layout_width="100dp"
                        android:layout_height="100dp"
                        android:layout_marginLeft="15dp"
                        android:background="@color/white"
                        android:scaleType="fitCenter"
                        app:srcCompat="@drawable/ic_launcher_background" />
                    <View
                        android:layout_width="136dp"
                        android:layout_height="10dp"
                        android:background="@color/white"/>
                    <TextView
                        android:id="@+id/fc_dog_food_name"
                        android:layout_width="100dp"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/maplestorylight"
                        android:layout_marginLeft="15dp"
                        android:textAlignment="center"
                        android:textSize="11dp"
                        android:text="그러니지 인섹트 솔루션 조인트 케어"
                        android:background="@color/white"
                        android:textColor="@color/black"/>
                    <View
                        android:layout_width="136dp"
                        android:layout_height="10dp"
                        android:background="@color/white"/>
                </LinearLayout>

                <TextView
                    android:id="@+id/fc_score_and_sales_title"
                    android:layout_width="wrap_content"
                    android:layout_height="16dp"
                    android:layout_marginTop="10dp"
                    android:fontFamily="@font/maplestorybold"
                    android:textColor="@color/black"
                    android:textSize="13dp"
                    android:layout_marginLeft="15dp"
                    android:text="평점 / 판매량" />

                <LinearLayout
                    android:id="@+id/fc_score_and_Sales_layout"
                    android:layout_width="134dp"
                    android:layout_height="wrap_content"
                    android:background="@color/white"
                    android:layout_marginTop="10dp"
                    android:orientation="vertical">
                    <View
                        android:layout_width="136dp"
                        android:layout_height="5dp"
                        android:background="@color/white"/>
                    <LinearLayout
                        android:layout_marginTop="5dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@color/white"
                        android:orientation="horizontal">
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:background="@color/white"
                            android:orientation="horizontal">

                            <ImageView
                                android:id="@+id/iv_star"
                                android:layout_width="12dp"
                                android:layout_height="15dp"
                                android:layout_marginLeft="15dp"
                                android:scaleType="centerCrop"
                                android:src="@drawable/star" />

                            <TextView
                                android:id="@+id/fc_score"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="5dp"
                                android:fontFamily="@font/maplestorybold"
                                android:text="5"
                                android:textColor="#FFC911"
                                android:textSize="13dp" />

                        </LinearLayout>
                    </LinearLayout>
                    <TextView
                        android:id="@+id/fc_sales_rate"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="5dp"
                        android:text="(2)"
                        android:fontFamily="@font/maplestorylight"
                        android:textColor="@color/black"
                        android:layout_marginLeft="15dp"
                        android:textSize="9dp"/>
                    <View
                        android:layout_width="136dp"
                        android:layout_height="5dp"
                        android:background="@color/white"/>

                </LinearLayout>

                <TextView
                    android:id="@+id/fc_manu_title"
                    android:layout_width="wrap_content"
                    android:layout_height="16dp"
                    android:layout_marginTop="10dp"
                    android:fontFamily="@font/maplestorybold"
                    android:textColor="@color/black"
                    android:textSize="13dp"
                    android:layout_marginLeft="15dp"
                    android:text="제조사" />
                <LinearLayout
                    android:layout_width="134dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:orientation="vertical"
                    android:background="@color/white">

                    <View
                        android:layout_width="136dp"
                        android:layout_height="10dp"
                        android:background="@color/white"/>

                    <TextView
                        android:id="@+id/fc_manu"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="15dp"
                        android:text="(주)펫큐베이션"
                        android:textSize="9dp"
                        android:fontFamily="@font/maplestorylight"
                        android:background="@color/white"
                        android:textColor="@color/black" />

                    <View
                        android:layout_width="136dp"
                        android:layout_height="10dp"
                        android:background="@color/white"/>

                </LinearLayout>
                <TextView
                    android:id="@+id/fc_calorie_title"
                    android:layout_width="wrap_content"
                    android:layout_height="16dp"
                    android:layout_marginTop="10dp"
                    android:fontFamily="@font/maplestorybold"
                    android:textColor="@color/black"
                    android:textSize="13dp"
                    android:layout_marginLeft="15dp"
                    android:text="칼로리" />

                <LinearLayout
                    android:layout_width="134dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:orientation="vertical"
                    android:background="@color/white">

                    <View
                        android:layout_width="136dp"
                        android:layout_height="10dp"
                        android:background="@color/white"/>

                    <TextView
                        android:id="@+id/fc_cal"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="15dp"
                        android:text="(250기준) 727.5kcal"
                        android:textSize="9dp"
                        android:fontFamily="@font/maplestorylight"
                        android:background="@color/white"
                        android:textColor="@color/black" />

                    <View
                        android:layout_width="136dp"
                        android:layout_height="10dp"
                        android:background="@color/white"/>

                </LinearLayout>
                <TextView
                    android:id="@+id/fc_material_title"
                    android:layout_width="wrap_content"
                    android:layout_height="16dp"
                    android:layout_marginTop="10dp"
                    android:fontFamily="@font/maplestorybold"
                    android:textColor="@color/black"
                    android:textSize="13dp"
                    android:layout_marginLeft="15dp"
                    android:text="주원료" />

                <LinearLayout
                    android:layout_width="134dp"
                    android:layout_height="40dp"
                    android:layout_marginTop="10dp"
                    android:orientation="vertical"
                    android:background="@color/white">

                    <View
                        android:layout_width="136dp"
                        android:layout_height="10dp"
                        android:background="@color/white"/>

                    <TextView
                        android:id="@+id/fc_material"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="15dp"
                        android:text="호박씨,해바라기씨,게껍질,
                        양고기"
                        android:textSize="9dp"
                        android:fontFamily="@font/maplestorylight"
                        android:background="@color/white"
                        android:textColor="@color/black" />

                    <View
                        android:layout_width="136dp"
                        android:layout_height="10dp"
                        android:background="@color/white"/>

                </LinearLayout>

                <TextView
                    android:id="@+id/fc_price_title"
                    android:layout_width="wrap_content"
                    android:layout_height="16dp"
                    android:layout_marginTop="10dp"
                    android:fontFamily="@font/maplestorybold"
                    android:textColor="@color/black"
                    android:textSize="13dp"
                    android:layout_marginLeft="15dp"
                    android:text="가격" />

                <LinearLayout
                    android:layout_width="134dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:orientation="vertical"
                    android:background="@color/white">

                    <View
                        android:layout_width="136dp"
                        android:layout_height="10dp"
                        android:background="@color/white"/>

                    <TextView
                        android:id="@+id/tv_fc_cost"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="15dp"
                        android:text="(100g당)1995원"
                        android:textSize="9dp"
                        android:fontFamily="@font/maplestorylight"
                        android:background="@color/white"
                        android:textColor="@color/black" />

                    <View
                        android:layout_width="136dp"
                        android:layout_height="10dp"
                        android:background="@color/white"/>

                </LinearLayout>


                <TextView
                    android:id="@+id/fc_nutrient_title"
                    android:layout_width="wrap_content"
                    android:layout_height="16dp"
                    android:layout_marginTop="10dp"
                    android:fontFamily="@font/maplestorybold"
                    android:textColor="@color/black"
                    android:textSize="13dp"
                    android:layout_marginLeft="15dp"
                    android:text="영양소" />

                <LinearLayout
                    android:layout_width="134dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:orientation="vertical"
                    android:background="@color/white">

                    <View
                        android:layout_width="136dp"
                        android:layout_height="10dp"
                        android:background="@color/white"/>

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">
                        <LinearLayout
                            android:layout_marginLeft="10dp"
                            android:layout_marginTop="5dp"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:orientation="vertical">
                            <TextView
                                android:id="@+id/fc_protein"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/maplestorylight"
                                android:textColor="@color/black"
                                android:textSize="9dp"
                                android:text="조단백질 23%"/>
                            <TextView
                                android:id="@+id/fc_fat"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/maplestorylight"
                                android:textColor="@color/black"
                                android:textSize="9dp"
                                android:text="조지방 8%"/>
                            <TextView
                                android:id="@+id/fc_calcium"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textColor="@color/black"
                                android:fontFamily="@font/maplestorylight"
                                android:textSize="9dp"
                                android:text="칼슘 0.8%"/>
                            <TextView
                                android:id="@+id/fc_fiber"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textColor="@color/black"
                                android:fontFamily="@font/maplestorylight"
                                android:textSize="9dp"
                                android:text="조섬유 8%"/>
                            <TextView
                                android:id="@+id/fc_ash"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textColor="@color/black"
                                android:fontFamily="@font/maplestorylight"
                                android:textSize="9dp"
                                android:text="조회분 6.5%"/>
                        </LinearLayout>

                        <LinearLayout
                            android:layout_marginLeft="10dp"
                            android:layout_marginTop="5dp"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:orientation="vertical">
                            <TextView
                                android:id="@+id/fc_phosphorus"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textColor="@color/black"
                                android:fontFamily="@font/maplestorylight"
                                android:textSize="9dp"
                                android:text="인 0.65%"/>
                            <TextView
                                android:id="@+id/fc_moisture"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textColor="@color/black"
                                android:fontFamily="@font/maplestorylight"
                                android:textSize="9dp"
                                android:text="수분 9.50%"/>
                            <TextView
                                android:id="@+id/fc_omega6"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textColor="@color/black"
                                android:fontFamily="@font/maplestorylight"
                                android:textSize="9dp"
                                android:text="오메가6 0%"/>
                            <TextView
                                android:id="@+id/fc_omega3"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textColor="@color/black"
                                android:fontFamily="@font/maplestorylight"
                                android:textSize="9dp"
                                android:text="오메가3 0.8%"/>
                            <TextView
                                android:id="@+id/fc_del"
                                android:layout_width="20dp"
                                android:layout_height="wrap_content"
                                android:text="DEL"
                                android:layout_marginTop="20dp"
                                android:layout_marginLeft="32dp"
                                android:textColor="#FF0000"
                                android:fontFamily="@font/maplestorylight"
                                android:textSize="11dp"/>

                        </LinearLayout>


                    </LinearLayout>

                    <View
                        android:layout_width="136dp"
                        android:layout_height="10dp"
                        android:background="@color/white"/>

                </LinearLayout>

            </LinearLayout>

        </LinearLayout>
    </LinearLayout>
</ScrollView>