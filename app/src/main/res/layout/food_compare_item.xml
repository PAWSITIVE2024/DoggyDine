<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="136dp"
    android:layout_height="match_parent"
    android:orientation="horizontal"
    android:background="@color/white">
    <LinearLayout
        android:layout_width="136dp"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <LinearLayout
            android:layout_width="136dp"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:background="@color/white">
            <ImageButton
                android:id="@+id/imageButton"
                android:layout_width="136dp"
                android:layout_height="147dp"
                android:background="@color/white"
                android:scaleType="fitCenter"
                app:srcCompat="@drawable/ic_launcher_background" />
            <View
                android:layout_width="136dp"
                android:layout_height="10dp"
                android:background="#f2f1e6"/>
            <TextView
                android:id="@+id/textView7"
                android:layout_width="136dp"
                android:layout_height="wrap_content"
                android:fontFamily="@font/maplestorybold"
                android:textAlignment="center"
                android:textSize="15dp"
                android:text="그러니지 인섹트 솔루션 조인트 케어"
                android:hint="사료이름"
                android:background="#f2f1e6"
                android:textColor="@color/black"/>
            <View
                android:layout_width="136dp"
                android:layout_height="10dp"
                android:background="#f2f1e6"/>
        </LinearLayout>

        <TextView
            android:id="@+id/textView13"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:fontFamily="@font/maplestorybold"
            android:textColor="@color/black"
            android:textSize="17dp"
            android:layout_marginLeft="15dp"
            android:text="평점 / 판매량" />

        <LinearLayout
            android:layout_width="136dp"
            android:layout_height="wrap_content"
            android:background="#f2f1e6"
            android:layout_marginTop="15dp"
            android:orientation="vertical">
            <View
                android:layout_width="136dp"
                android:layout_height="10dp"
                android:background="#f2f1e6"/>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#f2f1e6"
                android:orientation="horizontal">
                <ImageView
                    android:id="@+id/iv_start"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_marginLeft="10dp"
                    android:src="@drawable/star"/>
                <TextView
                    android:id="@+id/tv_fc_score"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/maplestorybold"
                    android:layout_marginLeft="5dp"
                    android:textSize="20dp"
                    android:layout_marginTop="5dp"
                    android:textColor="#FFC911"
                    android:text="5"
                    android:hint="5"/>

            </LinearLayout>
            <TextView
                android:id="@+id/tv_sales_rate"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:text="(2)"
                android:hint="(2)"
                android:fontFamily="@font/maplestorybold"
                android:textColor="@color/black"
                android:layout_marginLeft="16dp"/>
            <View
                android:layout_width="136dp"
                android:layout_height="10dp"
                android:background="#f2f1e6"/>

        </LinearLayout>

        <TextView
            android:id="@+id/textView14"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:fontFamily="@font/maplestorybold"
            android:textColor="@color/black"
            android:textSize="17dp"
            android:layout_marginLeft="15dp"
            android:text="제조사" />
        <LinearLayout
            android:layout_width="136dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp"
            android:orientation="vertical"
            android:background="#f2f1e6">

            <View
                android:layout_width="136dp"
                android:layout_height="10dp"
                android:background="#f2f1e6"/>

            <TextView
                android:id="@+id/tv_fc_manu"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="15dp"
                android:text="(주)펫큐베이션"
                android:textSize="15dp"
                android:fontFamily="@font/maplestorybold"
                android:background="#f2f1e6"
                android:textColor="@color/black" />

            <View
                android:layout_width="136dp"
                android:layout_height="10dp"
                android:background="#f2f1e6"/>

        </LinearLayout>
        <TextView
            android:id="@+id/textView15"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:fontFamily="@font/maplestorybold"
            android:textColor="@color/black"
            android:textSize="17dp"
            android:layout_marginLeft="15dp"
            android:text="칼로리" />

        <LinearLayout
            android:layout_width="136dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp"
            android:orientation="vertical"
            android:background="#f2f1e6">

            <View
                android:layout_width="136dp"
                android:layout_height="10dp"
                android:background="#f2f1e6"/>

            <TextView
                android:id="@+id/tv_fc_cal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="12dp"
                android:text="(250기준) 727.5kcal"
                android:textSize="13dp"
                android:fontFamily="@font/maplestorybold"
                android:background="#f2f1e6"
                android:textColor="@color/black" />

            <View
                android:layout_width="136dp"
                android:layout_height="10dp"
                android:background="#f2f1e6"/>

        </LinearLayout>
        <TextView
            android:id="@+id/textView16"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:fontFamily="@font/maplestorybold"
            android:textColor="@color/black"
            android:textSize="17dp"
            android:layout_marginLeft="15dp"
            android:text="주원료" />

        <LinearLayout
            android:layout_width="136dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp"
            android:orientation="vertical"
            android:background="#f2f1e6">

            <View
                android:layout_width="136dp"
                android:layout_height="10dp"
                android:background="#f2f1e6"/>

            <TextView
                android:id="@+id/tv_fc_material"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="12dp"
                android:text="곡류,닭고기,곤충"
                android:textSize="13dp"
                android:fontFamily="@font/maplestorybold"
                android:background="#f2f1e6"
                android:textColor="@color/black" />

            <View
                android:layout_width="136dp"
                android:layout_height="10dp"
                android:background="#f2f1e6"/>

        </LinearLayout>

        <TextView
            android:id="@+id/textView17"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:fontFamily="@font/maplestorybold"
            android:textColor="@color/black"
            android:textSize="17dp"
            android:layout_marginLeft="15dp"
            android:text="가격" />

        <LinearLayout
            android:layout_width="136dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp"
            android:orientation="vertical"
            android:background="#f2f1e6">

            <View
                android:layout_width="136dp"
                android:layout_height="10dp"
                android:background="#f2f1e6"/>

            <TextView
                android:id="@+id/tv_fc_coast"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="12dp"
                android:text="(100g당)1995원"
                android:textSize="13dp"
                android:fontFamily="@font/maplestorybold"
                android:background="#f2f1e6"
                android:textColor="@color/black" />

            <View
                android:layout_width="136dp"
                android:layout_height="10dp"
                android:background="#f2f1e6"/>

        </LinearLayout>
        <Button
            android:id="@+id/Btn_fc_delete"
            style="@style/Widget.AppCompat.Button.Colored"
            android:layout_width="62dp"
            android:layout_height="28dp"
            android:layout_marginLeft="70dp"
            android:layout_marginTop="30dp"
            android:background="@drawable/rounded_button"
            android:fontFamily="@font/maplestorybold"
            android:text="DEL"
            android:textColor="#000000"
            android:textSize="8dp" />
    </LinearLayout>
    <View
        android:layout_width="1dp"
        android:layout_height="match_parent"
        android:background="#D7D4E5">
    </View>





</LinearLayout>